  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:debug": "nest start --debug 0.0.0.0:9229",
    "test:watchAll:debug": "NODE_OPTIONS=--inspect=0.0.0.0:9231 jest --watchAll",
    "test:debug": "NODE_OPTIONS=--inspect-brk=0.0.0.0:9230 jest",
    "db:migrate:down": "sequelize-cli db:migrate:undo:all",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:setup": "npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^4.0.0",
